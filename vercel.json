{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTION"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}